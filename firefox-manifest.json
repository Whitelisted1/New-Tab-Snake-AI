{
    "manifest_version": 2,
    "name": "New Tab Snake AI",
    "version": "1.0.4",
    "description": "Replaces the default new tab page with an AI that plays Snake",
    
    "chrome_url_overrides" : {
        "newtab": "./assets/index.html"
      },

      "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "./popup/popup.html"
      },
      

      "permissions": [
        "storage"
      ]
  }