{
    "name": "New Tab Snake AI",
    "description": "Replaces the default new tab page with an AI that plays Snake",
    "icons": { "80": "icon.png"},
    "version": "1.0.4",
    "manifest_version": 3,
    "chrome_url_overrides": {
      "newtab": "./assets/index.html"
    },
    "action": {
      "default_popup": "./assets/popup.html"
    },
    "permissions": [
      "storage"
    ]
  }